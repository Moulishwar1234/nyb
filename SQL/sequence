SELECT
    *
FROM
    ALL_TABLES
WHERE
    TABLE_NAME IN ( 'EMPLOYEES', 'DEPARTMENTS' );

CREATE SYNONYM EMPLOYEES FOR HR.EMPLOYEES;

CREATE SYNONYM DEPARTMENTS FOR HR.DEPARTMENTS;

SELECT
    USER
FROM
    DUAL;

GRANT SELECT ON HR.EMPLOYEES TO YOUR_ACTUAL_USERNAME;

GRANT SELECT ON HR.DEPARTMENTS TO YOUR_ACTUAL_USERNAME;

SELECT
    E.EMPLOYEE_ID,
    E.FIRST_NAME,
    E.LAST_NAME,
    D.DEPARTMENT_NAME,
    D.LOCATION_ID
FROM
         HR.EMPLOYEES E
    JOIN HR.DEPARTMENTS D ON E.DEPARTMENT_ID = D.DEPARTMENT_ID;

    SELECT
    E.EMPLOYEE_ID,
    E.FIRST_NAME,
    E.LAST_NAME,
    D.DEPARTMENT_NAME,
    D.LOCATION_ID
FROM
    HR.EMPLOYEES E
FULL OUTER JOIN
    HR.DEPARTMENTS D ON E.DEPARTMENT_ID = D.DEPARTMENT_ID;
CREATE SEQUENCE "RK"."XXNYB_EMPLOYEES_NEW_SEQ" MINVALUE 1 MAXVALUE 999999999999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE
20 NOCYCLE NOORDER NOKEEP NOSCALE GLOBAL;


